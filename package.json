{
  "name": "nome",
  "version": "0.0.1",
  "description": "descricao",
  "author": "Indiara Camillo Menegat",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run format && npm run lint && tsc --noEmit -p . && rimraf dist && babel src --extensions \".ts,.js\" --out-dir dist --copy-files --no-copy-ignored --source-maps",
    "build:production": "babel src --extensions \".ts,.js\" --out-dir dist --copy-files --no-copy-ignored",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "debug": "node --inspect -r ts-node/register src/index.ts"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fastest-levenshtein": "^1.0.16",
    "mongoose": "^7.6.3",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": "20"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.6.3"
  }
}
